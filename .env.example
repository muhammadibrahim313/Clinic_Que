# Copy this file to `.env` and fill in your own values.

# PostgreSQL connection string.  Use SQLite by leaving this unset.
DATABASE_URL=postgresql://<username>:<password>@<host>/<dbname>

# Admin passcode for the board.  If unset, defaults to the value stored in the database.
ADMIN_PASS=demo

# Redis URL for event publication (optional).  Example: redis://localhost:6379/0
REDIS_URL=

# Twilio credentials (optional).  Used by workers/sms.py to send messages.
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# WhatsApp credentials (optional)
WHATSAPP_TOKEN=
WHATSAPP_PHONE_NUMBER=